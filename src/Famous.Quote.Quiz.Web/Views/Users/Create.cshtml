@model UserEditorModel
@{
    ViewData["Title"] = "Add New User";
}
@section Breadcrumbs
{
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <a href="@Url.RouteUrl(RouteNames.User.Users)">Users</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span>@ViewData["Title"]</span>
            </li>
        </ul>
    </div>
}

<form asp-controller="Users" asp-action="Create" method="post">
    <partial name="_UserCreateEditor.cshtml" model="Model.User" />
</form>